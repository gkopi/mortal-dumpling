 {% extends 'MortalDumplingBundle::base.html.twig' %}
{% block content %}
	{{parent()}} 
	<div id="container" class="js-isotope"
  data-isotope-options='{ "itemSelector": ".item" }'>
		{% for content_block in content.content_blocks %}
		    {{ include('MortalDumplingBundle:Dumpling:ContentBlock.html.twig') }}
		{% endfor %}
		
	</div>	
{% endblock content%}
{% block javascripts %}
  {{ parent() }}
   <script>
  	var container = document.querySelector('#container');
	var msnry;
	imagesLoaded( container, function() {
	  msnry = new Masonry( container );
	});
	</script>
{% endblock %}